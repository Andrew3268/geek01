<%= simple_form_for(@scategory) do |f| %>
  <%= f.error_notification %>
  <%= f.error_notification message: f.object.errors[:base].to_sentence if f.object.errors[:base].present? %>

  <div class="form-inputs">
    <%= f.input :scategory_01 %>
    <%= f.input :scategory_02 %>
    <%= f.input :scategory_03 %>
    <%= f.input :scategory_04 %>
    <%= f.input :scategory_05 %>
    <%= f.input :scategory_06 %>
    <%= f.input :scategory_07 %>
    <%= f.input :scategory_08 %>
    <%= f.input :scategory_09 %>
    <%= f.input :scategory_10 %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
